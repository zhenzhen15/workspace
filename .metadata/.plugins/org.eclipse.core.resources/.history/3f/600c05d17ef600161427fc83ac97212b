		System.out.println("163 mail login");
		
		WebDriver driver = new FirefoxDriver();
		
		 driver.get("http://mail.163.com");
		 driver.findElement(By.id("auto-id-1487493330549")).clear();
		 driver.findElement(By.id("auto-id-1487493330549")).sendKeys("janyzhen15");
		 driver.findElement(By.id("auto-id-1487493330552")).clear();
		 driver.findElement(By.id("auto-id-1487493330552")).sendKeys("Zong890105");
		 driver.findElement(By.id("loginBtn")).click();
		
